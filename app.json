{
  "expo": {
    "name": "ftravel",
    "slug": "ftravel",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo/logo_ftravel.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/logo/logo_ftravel.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "useNextNotificationsApi": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo/logo_ftravel.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ftravel",
      "googleServicesFile": "./google-services.json",
      "config": {
        "manifest": {
          "meta-data": {
            "com.google.firebase.messaging.default_notification_color": null,
            "com.google.firebase.messaging.default_notification_icon": null
          }
        }
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "@react-native-google-signin/google-signin",
      "@react-native-firebase/app",
      "expo-build-properties"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "b3b17e74-77df-42db-87b1-d26341d45071"
      }
    }
  }
}
